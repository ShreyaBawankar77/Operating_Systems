hello.c
#include <stdio.h>
int main()
{
    printf("Hello World!");
    return 0;
}


add.c
#include <stdio.h>
int main(){
    int a, b;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    printf("Sum = %d\n", a + b);
    return 0;
}

sub.c
#include <stdio.h>
int main(){
    int a, b;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    printf("Difference = %d\n",a-b);
    return 0;
}

mul.c
#include <stdio.h>
int main(){
    int a, b;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    printf("product = %d\n",a*b);
    return 0;
}
div.c
#include <stdio.h>
int main(){
    int a, b;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    printf("Difference = %d\n",a-b);
    return 0;
} 
while.c
#include <stdio.h>
int main(){
    int i = 1;
    while(i<=5){
         printf("%d\n", i);
         i++;
    }
    return 0;
}

for_loop.c
#include <stdio.h>
int main(){
    int i;
    for(i=1; i<=5; i++){
       printf("%d\n", i);
    }
    return 0;
}


leap.c
#include <stdio.h>
int main(){
    int year;
    printf("Enter year: ");
    scanf("%d", &year);
    if((year % 400 == 0) || (year%4 == 0 && year % 100 != 0))
         printf("Leap Year\n");
    else
         printf("Not a leap Year\n");
    return 0;
}
 
oddeven.c
#include <stdio.h>
int main(){
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);
    if(n%2 == 0)
       printf("Even number\n");
    else
       printf("Odd number\n");
    return 0;
}
switch_calc.c
#include <stdio.h>
int main(){
    int a,b,ch;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    printf("1.add 2.sub 3.mul 4.div\n");
    scanf("%d", &ch);
    if(ch == 1)
       printf("Add = %d\n", a+b);
    else if (ch == 2)
       printf("Sub = %d\n",a-b);
    else if (ch == 3)
       printf("Mul = %d\n", a*b);
    else if (ch == 4)
       printf("Div = %d\n", a/b);
    else
       printf("Invalid choice\n");
    return 0;
}

